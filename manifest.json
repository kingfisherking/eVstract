{
    "manifest_version": 3,
    "name": "eVstract",
    "version": "1.0",

    "permissions": [
        "activeTab",
        "scripting"
    ],

    "action": {
        "default_title": "Click to use!"
    },

    "background": {
        "service_worker": "background.js", "scripts": "thirdParty/jquery-3.7.1.min.js"
    }, 

    "content_scripts": [{
        "js":["thirdParty/jquery-3.7.1.min.js", "eVstract.js", "test.js"]
    }]

}